{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites":[
    {
      "source":"/api/(.*)",
      "destination":"https://api.timbu.cloud/$1"
    },
    {
      "source":"/(.*)",
      "destination":"/index.html"
    }
  ]
}
